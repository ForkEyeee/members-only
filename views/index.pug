extends layout

block content
	h1= title
	p Welcome to #{title}
	if JSON.stringify(currentUser) !== '{}' 
		p You are a User
	if currentUser.membership 
		p You are a Member
	if message_list && JSON.stringify(currentUser) !== '{}'
		each message in message_list
			if currentUser.membership
				ul
					li #{message.title} 
					li #{message.timestamp}
					li #{message.text}
			else 
				ul
					li #{message.text}





